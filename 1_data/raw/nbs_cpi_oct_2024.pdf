		$('li.dropdown.mega-dropdown').removeClass('open');



		if(pagging == "home") {
			if(localStorage.blocknot !== "true" && pagging == "home") {
				$('#notlist').addClass('nc-al open');			
				$('#allownottext').removeClass().addClass('hide');
				$('#blocknottext').removeClass().addClass('show');
